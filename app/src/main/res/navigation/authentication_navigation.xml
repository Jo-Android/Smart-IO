<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/authentication_navigation"
    app:startDestination="@id/loginFragment">

    <fragment
        android:id="@+id/loginFragment"
        android:name="com.hgtech.smartio.ui.fragment.user.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login">
        <action
            android:id="@+id/action_loginFragment_to_signUPFragment"
            app:destination="@id/signUPFragment" />
    </fragment>
    <fragment
        android:id="@+id/signUPFragment"
        android:name="com.hgtech.smartio.ui.fragment.user.SignUPFragment"
        android:label="fragment_sign_u"
        tools:layout="@layout/fragment_sign_u" />
</navigation>